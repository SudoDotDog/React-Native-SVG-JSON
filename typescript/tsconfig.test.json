{
	"compileOnSave": false,
	"compilerOptions": {
		"lib": [
			"dom",
			"esnext"
		],
		"jsx": "react-native",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"strict": true,
		"strictNullChecks": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"target": "es6"
	}
}